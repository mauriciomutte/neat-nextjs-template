{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:base"
	],
	"labels": ["dependencies"],
	"semanticCommits": true,
	"stabilityDays": 7,
	"prCreation": "not-pending",
	"packageRules": [
		{
			"packageNames": ["node"],
			"enabled": false
		},
		{
			"matchUpdateTypes": ["minor", "patch", "pin", "digest"],
			"automerge": true
		},
		{
			"matchDepTypes": ["devDependencies"],
			"automerge": true
		}
	]
}
